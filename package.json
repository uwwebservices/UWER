{
  "name": "idcard",
  "version": "0.1.0",
  "description": "UW WebServices Registration POC",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build": "babel src/backend -s -D -d dist/backend && webpack --config webpack.config.js",
    "start": "sh scripts/server_setup.sh && cross-env NODE_ENV=prod node dist/backend",
    "prod": "sh scripts/server_setup.sh && cross-env NODE_ENV=prod node dist/backend",
    "prestart": "npm run -s clean && npm run -s build",
    "clean": "rm -rf dist",
    "test": "eslint src && mocha --require babel-core/register test/*.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwwebservices/idcard-webapp-poc.git"
  },
  "author": "UWWebServices <ews-team@uw.edu>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-module-resolver": "^3.1.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "cross-env": "^5.1.5",
    "csv-express": "^1.2.2",
    "express": "^4.16.3",
    "material-ui": "^1.0.0-beta.41",
    "morgan": "^1.8.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "webpack": "^4.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "sass-loader": "^6.0.7",
    "should": "^13.2.1",
    "style-loader": "^0.21.0",
    "uglify-js": "^3.3.24",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1"
  }
}
